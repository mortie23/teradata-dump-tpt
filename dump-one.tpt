DEFINE JOB EXPORT_CUSTOMER
DESCRIPTION 'Export GAME table to a delimited file'
(
  DEFINE SCHEMA sch_CUSTOMER FROM TABLE 'PRD_ADS_BAC_UNITA_DB.game';
  APPLY TO OPERATOR ($FILE_WRITER()[1] ATTR (FileName = 'game.csv') )
  SELECT * FROM OPERATOR ($EXPORT(sch_CUSTOMER)[1] ATTR (
  SelectStmt = 'select * from PRD_ADS_BAC_UNITA_DB.game ;'
)
);
);
